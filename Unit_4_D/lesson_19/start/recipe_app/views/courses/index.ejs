<div class="container">
  <h2>Courses Table</h2>
  <table class="table">
    <thead>
      <tr>
        <th>Name</th>
        <th>Description</th>
      </tr>
    </thead>
    <tbody>
      <% courses.forEach(course => { %>
        <tr>
          <td>
            <%= course.name %>
          </td>
          <td>
            <%= course.description %>
          </td>
        </tr>
        <% }); %>
    </tbody>
  </table>
</div>
